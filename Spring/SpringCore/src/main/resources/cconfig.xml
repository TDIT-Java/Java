<?xml version="1.0" encoding="UTF-8"?>
<beans
        xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd
  ">


    <bean name="emp1" class="org.SpringCore.Collections.Emp">
        <property name="name" value="Pradeep"/>
        <property name="phoneNum">
            <list>
                <value>9850223872</value>
                <value>0987654321</value>
                <value>1234567890</value>
                <null />
            </list>
        </property>

        <property name="address">
            <set>
                <value>Pune</value>
                <value>Mumbai</value>
                <value>Delhi</value>
            </set>
        </property>

        <property name="courses">
            <map>
                <entry key="Java" value="6 Month"/>
                <entry key="Python" value="3 Month"/>
                <entry key="Nodejs" value="3 Month"/>
            </map>
        </property>
    </bean>

</beans>
